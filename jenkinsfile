pipeline {
    agent any


    stages {
        stage('SCM Checkout'){
          git 'https://github.com/sagardindorkar78/maven-project.git'
        }
  }
    {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'LocalMAVEN') {
                    sh 'mvn clean compile'
                }
            }
}
}
}
  
